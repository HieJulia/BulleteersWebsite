	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
<bean id="userDao" class="com.bulleteers.club.dao.UserDao" ></bean>
<bean id="memberDao" class="com.bulleteers.club.dao.MemberDao" ></bean>
<bean id="rideDao" class="com.bulleteers.club.dao.RideDao" ></bean>
<bean id="messageDao" class="com.bulleteers.club.dao.MessageDao" ></bean>
<bean id="dealerDao" class="com.bulleteers.club.dao.DealerDao" ></bean>
<bean id="productDao" class="com.bulleteers.club.dao.ProductDao" ></bean>
<bean id="memberValidator" class="com.bulleteers.club.validators.MemberValidator"></bean>
		
		
		
</beans>
